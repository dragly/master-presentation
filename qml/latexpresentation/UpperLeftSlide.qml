import QtQuick 2.0

Slide {
    anchors {
        top: parent.top
        left: parent.left
        right: parent.horizontalCenter
        bottom: parent.verticalCenter
        topMargin: parent.height * 0.03
        bottomMargin: parent.height * 0.03
        leftMargin: parent.width * 0.03
        rightMargin: parent.width * 0.03
    }
}
